{
    "version": "1.8.2",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "moduleResolution": "node"
    },
    "filesGlob": [
        "../typings/tsd-dev.d.ts",
        "**/*.ts",
        "!**/*.d.ts"
    ],
    "files": [
        "../typings/tsd-dev.d.ts",
        "test-Stream.ts",
        "test-Node.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
